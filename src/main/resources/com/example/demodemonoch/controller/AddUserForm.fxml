<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="293.0" prefWidth="214.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.demodemonoch.controller.AddUserFormController">
    <children>
        <GridPane alignment="CENTER_RIGHT" layoutX="7.0" layoutY="14.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Button fx:id="addButton" onAction="#register" mnemonicParsing="false" prefHeight="26.0"
                        prefWidth="166.0" text="Submit" GridPane.rowIndex="4"/>
                <Button mnemonicParsing="false" prefHeight="26.0" prefWidth="178.0" text="Cancel"
                        GridPane.columnIndex="1" GridPane.rowIndex="4"/>
                <Label GridPane.rowIndex="2"/>
                <Label GridPane.rowIndex="3"/>
                <TextField fx:id="id" promptText="id" GridPane.columnIndex="1"/>
                <TextField fx:id="firstName" promptText="firstname" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <TextField fx:id="lastName" promptText="lastname" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <TextField fx:id="position" promptText="position" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
                <Label prefHeight="18.0" prefWidth="90.0" text="Enter Id"/>
                <Label text="Enter firstName" GridPane.rowIndex="1"/>
                <Label text="Enter lastName" GridPane.rowIndex="2"/>
                <Label text="Enter position" GridPane.rowIndex="3"/>
            </children>
        </GridPane>
    </children>
</AnchorPane>
